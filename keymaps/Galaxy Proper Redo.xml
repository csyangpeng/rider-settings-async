<keymap version="1" name="Galaxy Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ActivateDatabaseToolWindow">
    <keyboard-shortcut first-keystroke="alt d" />
  </action>
  <action id="ActivateDebugToolWindow" />
  <action id="ActivateFindToolWindow" />
  <action id="ActivateQuickNotesToolWindow">
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="ActivateRunToolWindow">
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
  <action id="ActivateStructureToolWindow">
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="ActivateTasksToolWindow">
    <keyboard-shortcut first-keystroke="alt a" />
    <keyboard-shortcut first-keystroke="alt 5" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
  <action id="ActivateUnitTestsToolWindow">
    <keyboard-shortcut first-keystroke="alt u" />
    <keyboard-shortcut first-keystroke="alt 4" />
  </action>
  <action id="CollapseSelection" />
  <action id="EditorChooseLookupItemDot" />
  <action id="EditorDeleteLine" />
  <action id="Log.GoToNextError">
    <keyboard-shortcut first-keystroke="shift ctrl alt e" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="ctrl r" second-keystroke="ctrl r" />
  </action>
  <action id="Replace" />
  <action id="RiderUnitTestRunContextTwAction">
    <keyboard-shortcut first-keystroke="shift enter" />
  </action>
  <action id="SmartTypeCompletion">
    <keyboard-shortcut first-keystroke="ctrl period" />
    <keyboard-shortcut first-keystroke="shift ctrl space" />
  </action>
  <action id="ToggleDistractionFreeMode">
    <keyboard-shortcut first-keystroke="alt w" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="ctrl alt w" />
  </action>
</keymap>